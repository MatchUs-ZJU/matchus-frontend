		"@tarojs/mini-runner": "^3.4.3",
		"@tarojs/plugin-framework-react": "3.4.1",
		"@tarojs/react": "3.4.1",
		"@tarojs/runtime": "3.4.1",
		"@tarojs/taro": "3.4.12",
		"babel-plugin-import": "^1.13.3",
		"classname": "^0.0.0",
		"qs": "^6.10.3",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-redux": "^7.2.0",
		"react-transition-group": "^4.4.2",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.4.1",
		"@tarojs/webpack-runner": "3.4.12",
